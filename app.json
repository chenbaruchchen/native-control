{
  "expo": {
    "name": "HR-controls",
    "slug": "bikorot38",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/Appicons/playstore.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/Appicons/playstore.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "permissions":["READ_EXTERNAL_STORAGE","WRITE_EXTERNAL_STORAGE","INTERNET"],
      "adaptiveIcon": {
        "foregroundImage": "./assets/Appicons/playstore.png",
        "backgroundColor": "#ffff"
      },
      "package": "com.chenbaruch.bikorot"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
